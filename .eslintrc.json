{
  "extends": [
    "react-app",
    "react-app/jest"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "react-hooks/exhaustive-deps": "warn",
    "import/no-anonymous-default-export": "warn",
    "import/first": "error",
    "no-console": "off",
    "react/prop-types": "off"
  },
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true,
    "node": true
  }
}

